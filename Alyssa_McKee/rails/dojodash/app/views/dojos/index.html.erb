<h1>Listing <%= @dojos.count-1 %> Dojos</h1>

<table>
	<tr>
		<th>Codind Dojo Branch</th>
		<th>Steet</th>
		<th>City</th>
		<th>State</th>
		<th>Actions</th>
	</tr>
	<% @dojos.each do |dojo| %>
			<tr>
				<td><%= dojo.branch %></td>
				<td><%= dojo.street %></td>
				<td><%= dojo.city %></td>
				<td><%= dojo.state %></td>
				<td><a href="/dojos/<%= dojo.id %>">Show</a> | <a href="/dojos/<%= dojo.id %>/edit">Edit</a> | <a href="/dojos/<%= dojo.id %>/destroy" data-method="delete">Delete</a></td>
			</tr>
	<% end %>
	


</table>

<a href="/dojos/new">New Dojo</a>