<div id="container">
	<div id="content">
		<h1>All Users</h1>
		<table>
			<thead>
				<tr>
					<th>First Name</th>
					<th>Last Name</th>
					<th>Email</th>
					<th>Location</th>
					<th>State</th>
				</tr>
				
			</thead>
			<tbody>
				<% @users.each do |user| %>
					<tr>
						<td><%= user.first_name %></td>
						<td><%= user.last_name %></td>
						<td><%= user.email %></td>
						<td><%= user.location.city %></td>
						<td><%= user.location.state %></td>
					</tr>
				<% end %>
			</tbody>
		</table>
		
	</div>
</div>